<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
	<DataCollectionRunSettings>
		<DataCollectors>
			<DataCollector friendlyName="XPlat Code Coverage">
				<Configuration>
					<Format>opencover</Format>
					<ExcludeByFile>
						**/Program.cs
						**/Startup.cs
						**/*Migrations/*.cs
					</ExcludeByFile>
					<Exclude>
						[*.Tests]*
						[*].Migrations.*
						[Migrations]*
						[*]Configuration.*
						[*]TypeConfiguration
					</Exclude>
				</Configuration>
			</DataCollector>
		</DataCollectors>
	</DataCollectionRunSettings>
</RunSettings>
